{
  "name": "@colyseus/core",
  "version": "0.17.36",
  "description": "Multiplayer Framework for Node.js.",
  "type": "module",
  "input": "./src/index.ts",
  "main": "./build/index.cjs",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "@source": "./src/index.ts",
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.cjs"
    },
    "./*": {
      "@source": "./src/*.ts",
      "types": "./build/*.d.ts",
      "import": "./build/*.mjs",
      "require": "./build/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "funding": "https://github.com/sponsors/endel",
  "author": "Endel Dreyer",
  "license": "MIT",
  "keywords": [
    "colyseus",
    "multiplayer",
    "netcode",
    "realtime",
    "networking",
    "websockets"
  ],
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "homepage": "https://colyseus.io/",
  "engines": {
    "node": ">= 22.x"
  },
  "dependencies": {
    "@colyseus/msgpackr": "^1.11.2",
    "@colyseus/timer": "^2.0.0",
    "@standard-schema/spec": "^1.0.0",
    "debug": "^4.3.4",
    "nanoid": "^3.3.11",
    "@colyseus/greeting-banner": "^3.0.8",
    "@colyseus/shared-types": "^0.17.6",
    "@colyseus/better-call": "^1.3.1"
  },
  "devDependencies": {
    "@colyseus/schema": "^4.0.7",
    "express": "^5.0.0",
    "@colyseus/redis-presence": "^0.17.6",
    "@colyseus/redis-driver": "^0.17.6",
    "@colyseus/tools": "^0.17.18"
  },
  "peerDependencies": {
    "@colyseus/schema": "^4.0.7",
    "@pm2/io": "^6.1.0",
    "express": "^4.16.0 || ^5.0.0",
    "zod": "^4.1.12",
    "@colyseus/better-call": "^1.3.1",
    "@colyseus/ws-transport": "^0.17.9"
  },
  "optionalPeerDependencies": {
    "@colyseus/tools": "0.17.x",
    "@colyseus/redis-driver": "0.17.x",
    "@colyseus/redis-presence": "0.17.x"
  },
  "publishConfig": {
    "access": "public"
  },
